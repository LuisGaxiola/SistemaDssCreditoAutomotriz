datasource db {
  url      = env("PRISMA_DB_URL")
  provider = "sqlserver"
}
generator client {
  provider = "prisma-client-js"
}
model Auto {
  clave String @id @unique
  nombre String
  valorComercial Float
  plan Int
}
model Persona {
  curp String @id @unique
  nombre String
  domicilio String
}
model Plan {
  id Int @id @default(autoincrement())
  nombre String
  limiteCredito Float
  color String
}